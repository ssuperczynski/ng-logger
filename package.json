{
    "name": "@nsalaun/ng-logger",
    "version": "5.0.0-beta.1",
    "description": "Angular logger service",
    "main": "./bundles/ng-logger.umd.js",
    "module": "./modules/ng-logger.es5.js",
    "es2015": "./modules/ng-logger.js",
    "typings": "./ng-logger.d.ts",
    "scripts": {
        "build": "node build.js",
        "build:clean": "del-cli dist",
        "test": "karma start",
        "lint": "tslint --project '.' --type-check",
        "commit": "git-cz",
        "prerelease": "npm test",
        "release:git": "git add package.json && standard-version && git push --follow-tags origin master",
        "release:npm": "npm run build && npm publish dist",
        "release": "npm run release:git && npm run release:npm",
        "postrelease": "npm run build:clean",
        "codecov": "cat coverage/lcov.info | codecov"
    },
    "author": {
        "name": "Noémi Salaün",
        "email": "noemi.salaun@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/noemi-salaun/ng-logger.git"
    },
    "keywords": [
        "angular2",
        "logger",
        "typescript",
        "log",
        "ng",
        "angular"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/noemi-salaun/ng-logger/issues"
    },
    "homepage": "https://github.com/noemi-salaun/ng-logger#readme",
    "dependencies": {},
    "devDependencies": {
        "@angular/animations": "4.1.0",
        "@angular/common": "4.1.0",
        "@angular/compiler": "4.1.0",
        "@angular/compiler-cli": "4.1.0",
        "@angular/core": "4.1.0",
        "@angular/platform-browser": "4.1.0",
        "@angular/platform-browser-dynamic": "4.1.0",
        "@angular/platform-server": "4.1.0",
        "@types/jasmine": "2.6.0",
        "@types/node": "8.0.1",
        "chalk": "1.1.3",
        "codecov": "2.1.0",
        "codelyzer": "3.1.1",
        "commitizen": "2.9.6",
        "core-js": "2.4.1",
        "cz-conventional-changelog": "2.0.0",
        "del-cli": "1.1.0",
        "istanbul-instrumenter-loader": "3.0.0",
        "jasmine-core": "2.8.0",
        "karma": "1.7.1",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "1.3.0",
        "karma-jasmine": "1.1.0",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.31",
        "karma-webpack": "2.0.4",
