        "standard-version": "4.2.0",
        "ts-loader": "2.1.0",
        "tslint": "5.4.3",
        "tslint-config-standard": "^6.0.1",
        "tslint-loader": "3.5.3",
        "typescript": "2.3.4",
        "uglify-js": "2.8.29",
        "webpack": "3.0.0",
        "yargs": "8.0.2",
        "zone.js": "0.8.12"
